 (all,r,REG) MACSHK21_30(r,"usklab")= usklab21_30(r);
 (all,r,REG) MACSHK21_30(r,"sklab") = sklab21_30(r);
 (all,r,REG) MACSHK21_30(r,"cap")   = cap21_30(r);
 (all,r,REG) MACSHK21_30(r,"pop")   = pop21_30(r);

