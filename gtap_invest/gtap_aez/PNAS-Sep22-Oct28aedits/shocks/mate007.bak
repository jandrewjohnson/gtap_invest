 aorum14_21   from file SecFile02 header "N001";
 aonrum14_21  from file SecFile03 header "N001";
Formula 
 (all,r,REG) CRLVSHK14_21(r,"crops") = aocrop14_21(r);
 (all,r,REG) CRLVSHK14_21(r,"rum")   = aorum14_21(r);
 (all,r,REG) CRLVSHK14_21(r,"nrum")  = aonrum14_21(r);

