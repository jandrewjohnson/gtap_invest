 CROPLVSTK # Crops and livestock # (crops, rum, nrum);
Coefficient 
 (all,r,REG)(all,v,CROPLVSTK) CRLVSHK14_21(r,v) # Productivity shocks 2014-2021 #;
 (all,r,REG) aocrop14_21(r)  # ao for crops #;
 (all,r,REG) aorum14_21(r)   # ao for ruminant #;
 (all,r,REG) aonrum14_21(r)  # ao for non ruminant #;
