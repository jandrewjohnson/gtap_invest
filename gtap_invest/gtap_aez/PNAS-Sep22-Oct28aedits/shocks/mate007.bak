 (new) OutFile # output file #;
 GTAPSETS  # sets file #;
Set 
 REG    # regions # read elements from file GTAPSETS header "H1";
 MACVAR # macro variables # 
        (qgdp, aoreg, usklab, sklab, cap, pop);

Coefficient 
 (all,r,REG) qgdp14_21(r)   # gdp #;
 (all,r,REG) usklab14_21(r) # unskilled lab #;
 (all,r,REG) sklab14_21(r)  # skilled lab  #;
 (all,r,REG) cap14_21(r)    # capital #;
 (all,r,REG) pop14_21(r)    # population #;
Read
 qgdp14_21   from file MacFile01 header "N001";
 usklab14_21 from file MacFile02 header "N001";
 sklab14_21  from file MacFile03 header "N001";
 cap14_21    from file MacFile04 header "N001";
 pop14_21    from file MacFile05 header "N001";

Coefficient 
 (all,r,REG)(all,v,MACVAR) MACSHK14_21(r,v) # Macro shocks 2014-2021 #;
Formula 
 (all,r,REG)(all,v,MACVAR) MACSHK14_21(r,v) = 0.0;
 (all,r,REG) MACSHK14_21(r,"qgdp")  = qgdp14_21(r);
! (all,r,REG) MACSHK14_21("aoreg") = ;!
 (all,r,REG) MACSHK14_21(r,"usklab")= usklab14_21(r);
 (all,r,REG) MACSHK14_21(r,"sklab") = sklab14_21(r);
 (all,r,REG) MACSHK14_21(r,"cap")   = cap14_21(r);
 (all,r,REG) MACSHK14_21(r,"pop")   = pop14_21(r);

Coefficient 
 (all,r,REG)(all,v,MACVAR) MACSHK21_30(r,v) # Macro shocks 2021-2030 #;
 (all,r,REG) aoreg21_30(r)  # aoreg #;
 (all,r,REG) usklab21_30(r) # unskilled lab #;
 (all,r,REG) sklab21_30(r)  # skilled lab #;
 (all,r,REG) cap21_30(r)    # capital #;
 (all,r,REG) pop21_30(r)    # population #;
